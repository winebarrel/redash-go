{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard"
  ],
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "pgautoupgrade/pgautoupgrade"
      ],
      "matchManagers": [
        "docker-compose"
      ],
      "enabled": false
    }
  ]
}
